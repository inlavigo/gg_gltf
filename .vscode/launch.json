{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",

  "configurations": [
    {
      "name": "Run current file",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/bin/gg_create_package.dart",
      "args": [
        "cp",

        "-n",
        "aud_ab_space_widget",
        "-d",
        "aud_ab_space_widget renders several layers of an AbSpace efficiently.",
        "--no-open-source",
        "--no-cli",
        "--flutter",
        "--force"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Test Current File",
      "type": "dart",
      "request": "launch",
      "program": "${file}"
    }
  ]
}
